<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="gdi_rq_sequence" model="ir.sequence">
            <field name="name">GDI RQ Sequence</field>
            <field name="code">rental.quotation</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">4</field>
        </record>

        <record id="gdi_rental_contract_sequence" model="ir.sequence">
            <field name="name">GDI Rental Contract</field>
            <field name="code">rental.contract.seq</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">4</field>
        </record>

        <record id="gdi_rental_delivery_order_sequence" model="ir.sequence">
            <field name="name">GDI RDO Sequence</field>
            <field name="code">gdi.rental.delivery.order.seq</field>
            <field name="prefix">RDO%(y)s%(month)s</field>
            <field name="padding">4</field>
        </record>

    </data>

</odoo>
